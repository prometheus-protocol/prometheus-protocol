# Environment configuration for running 10 verifier bots
# Copy this to .env and replace with actual API keys

# Network configuration
IC_NETWORK=local  # Use 'local' for development, 'ic' for production
POLL_INTERVAL_MS=60000  # Poll every 60 seconds
BUILD_TIMEOUT_MS=600000  # 10 minute timeout per build

# API Keys for each verifier bot
# Generate these from the verifier dashboard after depositing stake
# Format: vr_<32-character-hex-string>

VERIFIER_1_API_KEY=vr_0123456789abcdef0123456789abcdef
VERIFIER_2_API_KEY=vr_0123456789abcdef0123456789abcdef
VERIFIER_3_API_KEY=vr_0123456789abcdef0123456789abcdef
VERIFIER_4_API_KEY=vr_0123456789abcdef0123456789abcdef
VERIFIER_5_API_KEY=vr_0123456789abcdef0123456789abcdef
VERIFIER_6_API_KEY=vr_0123456789abcdef0123456789abcdef
VERIFIER_7_API_KEY=vr_0123456789abcdef0123456789abcdef
VERIFIER_8_API_KEY=vr_0123456789abcdef0123456789abcdef
VERIFIER_9_API_KEY=vr_0123456789abcdef0123456789abcdef
VERIFIER_10_API_KEY=vr_0123456789abcdef0123456789abcdef


# GitHub token for downloading build dependencies
GITHUB_TOKEN=YOUR_GITHUB_TOKEN_HERE