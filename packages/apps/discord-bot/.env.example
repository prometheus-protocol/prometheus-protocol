# Discord Bot Environment Variables
# Copy this file to .env and replace placeholder values with your actual credentials

# Discord Configuration
DISCORD_TOKEN=your_discord_bot_token_here
DISCORD_CLIENT_ID=your_discord_client_id_here
DISCORD_GUILD_ID=your_guild_id_for_development # Optional, for testing

# Supabase Database Configuration
SUPABASE_URL=your_supabase_project_url
SUPABASE_SERVICE_ROLE_KEY=your_supabase_anon_key

# LLM Configuration
LLM_PROVIDER=openai # or anthropic
LLM_API_KEY=your_llm_api_key_here
LLM_MODEL=gpt-3.5-turbo # or gpt-4, claude-3-sonnet-20240229, etc.
LLM_MAX_TOKENS=1000
LLM_TEMPERATURE=0.7
LLM_TOOL_CONCURRENCY_LIMIT=3 # Max concurrent tool calls to prevent MCP server overload

# Alert Configuration
ALERT_DEFAULT_INTERVAL=300000 # 5 minutes in milliseconds
ALERT_MAX_CONCURRENT=10

OAUTH_CALLBACK_URL=http://localhost:8080/oauth/callback
