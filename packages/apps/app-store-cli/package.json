{
  "name": "@prometheus-protocol/app-store-cli",
  "version": "0.0.0-development",
  "description": "A CLI for publishing applications to the Prometheus App Store.",
  "type": "module",
  "bin": {
    "app-store-cli": "./dist/cli.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "node build.mjs",
