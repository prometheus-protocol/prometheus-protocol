    "@prometheus-protocol/declarations": "workspace:*",
    "@prometheus-protocol/ic-js": "workspace:*",
    "@prometheus-protocol/reproducible-build": "workspace:*",
    "commander": "^12.1.0",
    "js-yaml": "^4.1.0",
    "pem-file": "^1.0.1",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.12.12",
    "@types/prompts": "^2.4.9",
    "console-table-printer": "^2.14.6",
    "esbuild": "^0.25.9",
    "tsx": "^4.10.5",
    "typescript": "^5.4.5",
    "vitest": "^3.2.4"
  }
